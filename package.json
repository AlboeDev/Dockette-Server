{
  "name": "mms",
  "version": "0.0.0",
  "description": "Minecraft Managed Server",
  "main": "index.js",
  "scripts": {
    "build": "npm run clean && npm run transpile && npm run compile",
    "clean": "rimraf ./build ./dist ./temp",
    "clean:build": "rimraf ./build ./dist",
    "clean:compile": "rimraf ./build",
    "clean:npm": "rimraf ./node_modules",
    "clean:transpile": "rimraf ./dist",
    "compile": "rollup -c ./rollup.config.js",
    "lint": "eslint",
    "start": "node ./scripts/start.js",
    "test": "jest",
    "transpile": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/InteractiveTimmy/mms.git"
  },
  "author": "Alboe Development",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/InteractiveTimmy/mms/issues"
  },
  "homepage": "https://github.com/InteractiveTimmy/mms#readme",
  "dependencies": {
    "shelljs": "^0.8.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^9.1.2",
    "@commitlint/config-conventional": "^9.1.2",
    "@types/shelljs": "^0.8.8",
    "@typescript-eslint/eslint-plugin": "^4.0.1",
    "@typescript-eslint/parser": "^4.0.1",
    "eslint": "^7.8.1",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^23.20.0",
    "eslint-plugin-jsdoc": "^30.3.1",
    "husky": "^4.2.5",
    "jest": "^26.4.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.26.9",
    "typescript": "^4.0.2"
  }
}
